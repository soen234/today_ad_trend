{
  "expo": {
    "name": "TAT",
    "slug": "ad-trend-temp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1929"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.elehedge.tat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "This app uses tracking to show you personalized ads that match your interests."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a1929"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.elehedge.tat",
      "permissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8143178103770527~2055004965",
          "iosAppId": "ca-app-pub-8143178103770527~2055004965"
        }
      ],
      "expo-tracking-transparency",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow TAT to save ad screenshots to your photo library.",
          "savePhotosPermission": "Allow TAT to save ad screenshots to your photo library."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "733ddff6-69a7-4681-97be-002cd4f2233b"
      }
    },
    "owner": "elehedge"
  }
}
